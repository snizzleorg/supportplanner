{
  "name": "supportplanner",
  "version": "0.4.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node server.js",
    "start": "NODE_ENV=production node server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:frontend": "vitest run --config vitest.config.frontend.js",
