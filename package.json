    "test:frontend:coverage": "vitest run --coverage --config vitest.config.frontend.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.1",
    "express-session": "^1.17.3",
    "express-validator": "^7.2.0",
    "helmet": "^8.0.0",
    "ical-expander": "^3.1.0",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.2",
    "openid-client": "^5.6.5",
    "tsdav": "^2.1.2",
    "yaml": "^2.4.2"
  },
  "devDependencies": {
    "vitest": "^1.0.4",
    "@vitest/coverage-v8": "^1.0.4",
    "supertest": "^6.3.3",
    "jsdom": "^23.0.0"
  }
